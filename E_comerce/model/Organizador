package model;

public class Organizador {
	private String nome, razaoSocial, endereco, cnpj, responsavel, telefone;

	@Override
	public String toString() {
		return "Organizador [nome=" + nome + ", rezaoSocial=" + razaoSocial + ", endereco=" + endereco + ", cnpj="
				+ cnpj + ", responsavel=" + responsavel + ", telefone=" + telefone + "]";
	}

	


	public Organizador(String nome, String rezaoSocial, String endereco, String cnpj, String responsavel, String telefone) {
		super();
		this.nome = nome;
		this.razaoSocial = rezaoSocial;
		this.endereco = endereco;
		this.cnpj = cnpj;
		this.responsavel = responsavel;
		this.telefone = telefone;
	}
	
	public Organizador() {}

	public String getNome() {
		return nome;
	}

	public void setNome(String nome) {
		this.nome = nome;
	}

	public String getRezaoSocial() {
		return razaoSocial;
	}

	public void setRezaoSocial(String rezaoSocial) {
		this.razaoSocial = rezaoSocial;
	}

	public String getEndereco() {
		return endereco;
	}

	public void setEndereco(String endereco) {
		this.endereco = endereco;
	}

	public String getCnpj() {
		return cnpj;
	}

	public void setCnpj(String cnpj) {
		this.cnpj = cnpj;
	}

	public String getResponsavel() {
		return responsavel;
	}

	public void setResponsavel(String responsavel) {
		this.responsavel = responsavel;
	}	
	
	public String getTelefone() {
		return telefone;
	}
	
	public void setTelefone(String telefone) {
		this.telefone = telefone;
	}
	
}
